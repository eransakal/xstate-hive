{
  "name": "xstate-hive",
  "version": "0.0.0",
  "description": "An opinionated application state generator with XState for React application",
  "author": "Eran Sakal @eransakal",
  "bin": {
    "xstate-hive": "./bin/run.js"
  },
  "homepage": "https://github.com/eransakal/xstate-hive",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "eransakal/xstate-hive",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/bundle"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "evcodeshift": "^2.4.0",
    "node-plop": "^0.31.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.22.2",
    "@babel/register": "^7.21.0",
    "@oclif/test": "^2.3.17",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.1",
    "@types/jscodeshift": "^0.11.6",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.25",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "patch-package": "^7.0.0",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "xstate-hive",
    "dirname": "xstate-hive",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "machine": {
        "description": "Generate a new XState machine"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postinstall": "patch-package",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/eransakal/xstate-hive/issues",
  "keywords": [
    "React",
    "XState",
    "Generator"
  ],
  "types": "dist/index.d.ts",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
